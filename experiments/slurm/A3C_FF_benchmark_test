#!/usr/bin/env bash
#SBATCH --time=00:10:00
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=12
#SBATCH --job-name=A3C_FF_v07
#SBATCH --mail-type ALL
#SBATCH --mail-user jos.vandewolfshaar@gmail.com
#SBATCH --output job-%j.log
#SBATCH --mem=2000
#SBATCH --partition=gpu
#SBATCH --gres=gpu:k40:2

module load tensorflow/0.10.0-foss-2016a-Python-3.5.1

srun python mproj/deeprl/rlmethods/a3c.py --model a3c_ff --n_threads 1 --clip_rewards --T_max 100
srun python mproj/deeprl/rlmethods/a3c.py --model a3c_ff --n_threads 2 --clip_rewards --T_max 100
srun python mproj/deeprl/rlmethods/a3c.py --model a3c_ff --n_threads 4 --clip_rewards --T_max 100
srun python mproj/deeprl/rlmethods/a3c.py --model a3c_ff --n_threads 8 --clip_rewards --T_max 100
srun python mproj/deeprl/rlmethods/a3c.py --model a3c_ff --n_threads 12 --clip_rewards --T_max 100